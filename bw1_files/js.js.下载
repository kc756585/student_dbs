var AN=function(s){
	var k=['-moz-','-webkit-','-ms-','-o-'],y=s.replace(/(-bom-)/g,'');
	for(i in k){
		y+='\n'+s.replace(/(-bom-)/g,k[i]);
	}
	return y+'\n';
}
